<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="edu.nu.jam.whiteboard">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity
            android:name="edu.nu.jam.whiteboard.ReplyToCommentActivity"
            android:screenOrientation="portrait"
            android:label="Reply To Comment"
            android:parentActivityName="edu.nu.jam.whiteboard.NavDrawerActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="edu.nu.jam.whiteboard.NavDrawerActivity" />
        </activity>
        <activity
            android:name="edu.nu.jam.whiteboard.InitialSurveyActivity"
            android:screenOrientation="portrait"
            android:label="Learning Style Summary"
            android:parentActivityName="edu.nu.jam.whiteboard.NavDrawerActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="edu.nu.jam.whiteboard.NavDrawerActivity" />
        </activity>
        <activity
            android:name="edu.nu.jam.whiteboard.MajorityConcernActivity"
            android:screenOrientation="portrait"
            android:label="Majority Concern"
            android:parentActivityName="edu.nu.jam.whiteboard.NavDrawerActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="edu.nu.jam.whiteboard.NavDrawerActivity" />
        </activity>
        <activity
            android:name="edu.nu.jam.whiteboard.PasswordRecoveryActivity"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="adjustResize|stateHidden"
            android:label="Password Recovery"
            android:parentActivityName="edu.nu.jam.whiteboard.MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="edu.nu.jam.whiteboard.MainActivity"/>
        </activity>
        <activity
            android:name="edu.nu.jam.whiteboard.PasswordResetActivity"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="adjustResize|stateHidden"
            android:label="Password Reset"
            android:parentActivityName="edu.nu.jam.whiteboard.MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="edu.nu.jam.whiteboard.MainActivity"/>
        </activity>
        <activity
            android:name="edu.nu.jam.whiteboard.SessionSelectionActivity"
            android:screenOrientation="portrait"
            android:label="Session Selection"
            android:parentActivityName="edu.nu.jam.whiteboard.NavDrawerActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="edu.nu.jam.whiteboard.NavDrawerActivity" />
        </activity>
        <activity
            android:name="edu.nu.jam.whiteboard.SurveySelectionActivity"
            android:screenOrientation="portrait"
            android:label="Survey Selection"
            android:parentActivityName="edu.nu.jam.whiteboard.NavDrawerActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="edu.nu.jam.whiteboard.NavDrawerActivity" />
        </activity>
        <activity
            android:name="edu.nu.jam.whiteboard.ViewRepliesActivity"
            android:screenOrientation="portrait"
            android:label="Comment Replies"
            android:parentActivityName="edu.nu.jam.whiteboard.NavDrawerActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="edu.nu.jam.whiteboard.NavDrawerActivity" />
        </activity>
        <activity
            android:name="edu.nu.jam.whiteboard.ProgressiveSurveyActivity"
            android:screenOrientation="portrait"
            android:label="Weekly Learning Objectives"
            android:parentActivityName="edu.nu.jam.whiteboard.NavDrawerActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="edu.nu.jam.whiteboard.NavDrawerActivity" />
        </activity>
        <activity
            android:name="edu.nu.jam.whiteboard.NewCommentActivity"
            android:screenOrientation="portrait"
            android:label="New Comment"
            android:parentActivityName="edu.nu.jam.whiteboard.NavDrawerActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="edu.nu.jam.whiteboard.NavDrawerActivity" />
        </activity>

        <activity android:name="edu.nu.jam.whiteboard.NavDrawerActivity" />
        <activity
            android:name="edu.nu.jam.whiteboard.MainActivity"
            android:theme="@style/AppTheme.NoActionBar"
            android:windowSoftInputMode="adjustResize|stateHidden"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>

        <activity
            android:name="edu.nu.jam.whiteboard.SplashScreenActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />

    </application>

    <uses-permission android:name="android.permission.INTERNET" />

</manifest>